/*
 *  C Program to print ASCII value of a character
 */
#include <stdio.h>
#include <conio.h>
 
int main() {
    char c;
    printf("Enter a Character\n");
    scanf("%c",&c);
    /*Prints the ASCII value of character as integer */
    printf("ASCII value of %c = %d",c,c);
     
    getch();
    return 0;
}
